{
  "base_url": "http://127.0.0.1:8026",

  "LotCommandDownload": {
    "name": "/LotCommandDownload",
    "method": "POST",
    "direction": "push",
    "enabled": true,
    "enableHeader": true,
    "enableBody": true,
    "timeoutMs": 5000,
    "retryCount": 0,

    "header_mapping": {
    },

    "body_mapping": {
      "port_no": "port_no",
      "port_type": "port_type",
      "carrier_id": "carrier_id",
      "port_command": "port_command",
      "lot_infos.lot": "lot_infos.lot"
    },

    "response_mapping": {
      "header.function_name": "function_name",
      "header.eqp_id": "eqp_id",
      "header.time_stamp": "time_stamp",
      "header.trx_id": "trx_id"
    }
  }
}
