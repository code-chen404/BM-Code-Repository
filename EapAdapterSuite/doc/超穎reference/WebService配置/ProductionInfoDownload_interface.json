{
  "base_url": "http://127.0.0.1:8026",

  "ProductionInfoDownload": {
    "name": "/ProductionInfoDownload",
    "method": "POST",
    "direction": "push",
    "enabled": true,
    "enableHeader": true,
    "enableBody": true,
    "timeoutMs": 5000,
    "retryCount": 0,

    "header_mapping": {
    },

    "body_mapping": {
      "download_type": "download_type",
      "lot_id": "lot_id",
      "prod_id": "prod_id",
      "lot_short_id": "lot_short_id",
      "lot_qty": "lot_qty",
      "prod_version": "prod_version",
      "process_code": "process_code",
      "use_in_name": "use_in_name",
      "layer": "layer",
      "step_seq": "step_seq",
      "route_type": "route_type",
      "rework_route_id": "rework_route_id",
      "rework_step_seq": "rework_step_seq",
      "pnl_side": "pnl_side",
      "spec_seq": "spec_seq",
      "spec_control": "spec_control",
      "lot_infos.lot": "lot_infos.lot"
    },

    "response_mapping": {
      "header.function_name": "function_name",
      "header.eqp_id": "eqp_id",
      "header.time_stamp": "time_stamp",
      "header.trx_id": "trx_id",
      "body.lot_id": "lot_id"
    }
  }
}
