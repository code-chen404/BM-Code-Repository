    "enabled": true,
    "enableHeader": true,
    "enableBody": true,
    "timeoutMs": 5000,
    "retryCount": 0,

    "header_mapping": {
    },

    "body_mapping": {
      "carrier_id": "carrier_id",
      "carrier_seq": "carrier_seq",
      "port_type": "port_type",
      "port_no": "port_no",
      "prod_mode": "prod_mode",
      "production_mode": "production_mode",
