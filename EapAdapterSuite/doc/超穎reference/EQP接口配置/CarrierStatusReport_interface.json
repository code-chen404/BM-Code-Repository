{
  "base_url": "http://127.0.0.1:8026",

  "CarrierStatusReport": {
    "name": "/CarrierStatusReport",
    "method": "POST",
    "direction": "push",
    "enabled": true,
    "enableHeader": true,
    "enableBody": true,
    "timeoutMs": 5000,
    "retryCount": 0,

    "header_mapping": {
    },

    "body_mapping": {
      "carrier_id": "carrier_id",
      "carrier_seq": "carrier_seq",
      "carrier_status": "carrier_status",
      "prod_mode": "prod_mode",
      "lot_infos.lot": "lot_infos.lot"
    },

    "response_mapping": {
      "header.function_name": "function_name",
      "header.eqp_id": "eqp_id",
      "header.time_stamp": "time_stamp",
      "header.trx_id": "trx_id"
    }
  }
}
