{
  "base_url": "http://127.0.0.1:8026",

  "RealStatusReport": {
    "name": "/RealStatusReport",
    "method": "POST",
    "direction": "push",
    "enabled": true,
    "enableHeader": true,
    "enableBody": true,
    "timeoutMs": 5000,
    "retryCount": 0,

    "header_mapping": {
    },

    "body_mapping": {
      "cim_mode": "cim_mode",
      "auto_mode": "auto_mode",
      "emergency_mode": "emergency_mode",
      "status_id": "status_id",
      "alarm_id": "alarm_id",
      "alarm_desc": "alarm_desc",
      "user_id": "user_id",
      "upstream_connect_status": "upstream_connect_status",
      "downstream_connect_status": "downstream_connect_status",
      "lot_id": "lot_id",
      "prod_mode": "prod_mode",
      "prod_mix_mode": "prod_mix_mode",
      "is_stop_receive": "is_stop_receive",
      "program_version": "program_version",
      "idle_delay_time": "idle_delay_time",
      "queue_lot_count": "queue_lot_count"
    },

    "response_mapping": {
      "header.function_name": "function_name",
      "header.eqp_id": "eqp_id",
      "header.time_stamp": "time_stamp",
      "header.trx_id": "trx_id"
    }
  }
}
