{
  "base_url": "http://127.0.0.1:8026",

  "AlarmReport": {
    "name": "/AlarmReport",
    "method": "POST",
    "direction": "push",
    "enabled": true,
    "enableHeader": true,
    "enableBody": true,
    "timeoutMs": 5000,
    "retryCount": 0,

    "header_mapping": {
    },

    "body_mapping": {
      "report_dt": "report_dt",
      "keep_reason": "keep_reason",
      "alarm_type": "alarm_type",
      "alarm_code": "alarm_code",
      "alarm_id": "alarm_id",
      "alarm_desc": "alarm_desc",
      "status_id": "status_id"
    },

    "response_mapping": {
      "header.function_name": "function_name",
      "header.eqp_id": "eqp_id",
      "header.time_stamp": "time_stamp",
      "header.trx_id": "trx_id"
    }
  }
}
