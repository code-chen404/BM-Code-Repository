{
  "base_url": "http://127.0.0.1:8026",

  "UtilityReport": {
    "name": "/UtilityReport",
    "method": "POST",
    "direction": "push",
    "enabled": true,
    "enableHeader": true,
    "enableBody": true,
    "timeoutMs": 5000,
    "retryCount": 0,

    "header_mapping": {
    },

    "body_mapping": {
      "report_date": "report_date",
      "keep_reason": "keep_reason",
      "shift_id": "shift_id",
      "utl_infos.utl": "utl_infos.utl"
    },

    "response_mapping": {
      "header.function_name": "function_name",
      "header.eqp_id": "eqp_id",
      "header.time_stamp": "time_stamp",
      "header.trx_id": "trx_id"
    }
  }
}
