  "CIMMessage": {
    "name": "CIMMessage",
    "method": "POST",
    "direction": "push",
    "enabled": true,
    "enableHeader": true,
    "enableBody": true,
    "timeoutMs": 5000,
    "retryCount": 0,
    "header_mapping": {
      "function_name": "function_name",
      "eqp_id": "eqp_id",
      "time_stamp": "time_stamp",
      "trx_id": "trx_id"
    },
    "body_mapping": {
      "screen_control": "screen_control",
      "screen_code": "screen_code",
      "interval_second_time": "interval_second_time",
      "cim_msg": "cim_msg"
    },
    "response_mapping": {
      "header.function_name": "function_name",
      "header.trx_id": "trx_id",
      "header.result": "result",
      "header.rtn_code": "rtn_code",
      "header.rtn_msg": "rtn_msg"
    }
  },

